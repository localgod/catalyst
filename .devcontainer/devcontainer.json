{
	"name": "Ona",
	"build": {
        "context": ".",
        "dockerfile": "Dockerfile"
    },
	"features": {
		"ghcr.io/devcontainers/features/docker-in-docker": {
			"moby": false
		},
		"ghcr.io/devcontainers/features/node": {
			"nodeGypDependencies": true,
			"version": "lts",
			"nvmVersion": "latest"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"jebbs.plantuml",
				"redhat.vscode-xml",
				"DavidAnson.vscode-markdownlint",
				"hediet.vscode-drawio",
				"usernamehw.errorlens"
			]
		}
	}
}
